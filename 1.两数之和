class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int> ans;
        for(int i=0;i<=nums.size()-1;i++){
            if(ans.find(target-nums[i])!=ans.end())
                return {ans.find(target-nums[i])->second,i};
            ans[nums[i]]=i;
        }
        return {};
    }
};
